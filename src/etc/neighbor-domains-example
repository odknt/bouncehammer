# $Id: neighbor-domains-example,v 1.7 2010/07/11 06:49:34 ak Exp $
#  neighbor-domains:
#    Kanadzuchi::Mail::Group::Neighbor reads this file(YAML). If you use 
#    this file, it should be saved as 'neighbor-domains'.
#
# SYNOPSIS:
#  provider-name: [ 'domain1', 'domain2', ... ]
#
# EXAMPLE:
snakeoil: [ 'snakeoil-example.com', 'snakeoil-example.net' ]
oursites: 
  - 'snake.example.jp'
  - 'hebi.example.jp'
  - 'nyoro.example.jp'
  - 'daija.example.jp'

...
